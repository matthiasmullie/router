{  "name": "matthiasmullie/router",  "type": "library",  "description": "PSR-15 based router interface on top of FastRoute",  "keywords": [    "router", "psr-15", "fastroute", "http", "middleware"  ],  "homepage": "https://github.com/matthiasmullie/router",  "license": "MIT",  "authors": [    {      "name": "Matthias Mullie",      "homepage": "https://www.mullie.eu",      "email": "router@mullie.eu",      "role": "Developer"    }  ],  "require": {    "php": "^8.1",    "ext-json": "*",    "nikic/fast-route": "^1.3.0",    "psr/log": "^1.0 || ^2.0 || ^3.0",    "psr/http-message": "^1.0",    "psr/http-message-implementation": "^1.0 || ^2.0",    "psr/http-server-handler": "^1.0",    "psr/http-server-middleware": "^1.0"  },  "require-dev": {    "friendsofphp/php-cs-fixer": ">=3.0",    "guzzlehttp/psr7": "^2.0",    "monolog/monolog": "^3.0",    "phpunit/phpunit": ">=9.0"  },  "provides": {    "psr/http-server-handler-implementation": "^1.0",    "psr/http-server-middleware-implementation": "^1.0"  },  "suggest": {    "psr/log-implementation": "Log router events"  },  "autoload": {    "psr-4": {      "MatthiasMullie\\Router\\": "src/"    }  },  "autoload-dev": {    "psr-4": {      "MatthiasMullie\\Router\\Tests\\": "tests/"    }  },  "config": {    "allow-plugins": {      "php-http/discovery": true    }  }}